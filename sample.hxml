--macro nullSafety("glfw", Strict)
--macro include("glfw")
extraParams.hxml
--dce no
--main sample.Main
--debug
--cpp sample/build/
--xml documentation/cpp.xml
--cmd haxelib run dox -o documentation/out/ -i documentation/cpp.xml -in glfw --title "hxglfw API reference" --toplevel-package glfw --keep-field-order -D version 0.0.1 -D source-path https://github.com/ibilon/hxglfw/blob/master/ -D website https://github.com/ibilon/hxglfw -D description "hxglfw is a work in progress haxe/hxcpp wrapper around glfw"
